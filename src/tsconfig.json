{
  "compileOnSave": true,
  "compilerOptions": {
    "importHelpers": true,
    "target": "es6",
    "module": "commonjs",
    "outDir": "../out",
    "moduleResolution": "node",
    "sourceMap": true,
    "removeComments": true,
    "preserveConstEnums": true,
    "strictNullChecks": false,
    "resolveJsonModule": true,
    "skipLibCheck": true,
    "allowJs": true
  }
}